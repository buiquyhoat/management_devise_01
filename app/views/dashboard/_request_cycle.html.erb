  <div id="request_cycle">
    <div class="x_panel tile fixed_height_320 overflow_hidden">
      <div class="x_title">
        <h3>
          <%= t "dashboard.request.request_overview" %>
          <%= link_to t("dashboard.request.to_request"), requests_path %>
        </h3>
        <ul class="nav navbar-right panel_toolbox">
          <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>
      <div class="x_content">
      <% if @chartobj.present? && @chartobj[:have_data] %>
        <table class="chart w-100">
          <tr>
            <th class="w-37">
              <p><%= t "dashboard.request.chart" %></p>
            </th>
            <th>
              <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                <p class=""><%= t "dashboard.request.status" %></p>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                <p class=""><%= t "dashboard.request.summany" %></p>
              </div>
            </th>
          </tr>
          <tr>
            <td>
              <canvas id="canvas1"></canvas>
            </td>
            <td>
              <table class="tile_info">
                <% @chartobj[:labels].count.times do |i| %>
                  <tr>
                    <td>
                      <p>
                        <i class="fa fa-square <%= @chartobj[:colors][i] %>"></i>
                        <%= link_to @chartobj[:labels][i], requests_path(request_status_id: @chartobj[:request_status][i]) %>
                        </p>
                    </td>
                    <td><%= @chartobj[:datapercent][i] %>%</td>
                  </tr>
                <% end %>
              </table>
            </td>
          </tr>
        </table>
      <% else %>
        <div class="tile_info">
          <%= link_to t("dashboard.request.go_to_request"),  requests_path %>
        </div>
      <% end %>
      </div>
    </div>
  </div>
