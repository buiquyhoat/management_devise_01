<% if logged_in? %>
  <div class="row">
  <%= render "chart_overview" %>
  </div>
  <div class="row">
  <%= render "top_overview" %>
  </div>
  <% if @current_user.is_admin %>
    <div class="row">
      <%= render "admin_overview" %>
    </div>
  <% end %>
<% else %>
  <div class="center jumbotron">
    <h1><%= t "home.title" %></h1>
    <h2><%= t "home.content" %></h2>
  </div>
<% end %>
